<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vBox" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.teamOne.cs631.controller.RevenueEventDialogController">
    <AnchorPane fx:id="anchorPane" prefHeight="409.0" prefWidth="600.0">
        <children>
            <TextField fx:id="revenueTypeIdTextField" editable="false" layoutX="130.0" layoutY="34.0" />
            <Label layoutX="22.0" layoutY="38.0" prefHeight="18.0" prefWidth="93.0" text="Revenue Type ID" />



            <DatePicker fx:id="dateTimeTextField" editable="false" layoutX="130.0" layoutY="71.0" />
            <Label layoutX="22.0" layoutY="75.0" prefHeight="18.0" prefWidth="85.0" text="Date Time" />


            <TextField fx:id="ticketsSoldTextField" editable="false" layoutX="130.0" layoutY="107.0" />
            <Label layoutX="22.0" layoutY="109.0" prefHeight="18.0" prefWidth="75.0" text="Tickets Sold" />

            <TextField fx:id="revenueTextField" editable="false" layoutX="130.0" layoutY="141.0" />
            <Label layoutX="22.0" layoutY="145.0" prefHeight="18.0" prefWidth="85.0" text="Revenue" />



            <RadioButton fx:id="insertRadioBtn" layoutX="329.0" layoutY="175.0" mnemonicParsing="false" text="Insert">
                <toggleGroup>
                    <ToggleGroup fx:id="group" />
                </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="updateRadioBtn" layoutX="329.0" layoutY="197.0" mnemonicParsing="false" text="Update" toggleGroup="$group" />
            <RadioButton fx:id="viewRadioBtn" layoutX="329.0" layoutY="220.0" mnemonicParsing="false" text="View" toggleGroup="$group" />
            <Button fx:id="closeButton" alignment="CENTER" layoutX="224.0" layoutY="225.0" mnemonicParsing="false" text="Close" textAlignment="CENTER" />
            <Button fx:id="submitBtn" disable="true" layoutX="97.0" layoutY="225.0" mnemonicParsing="false" text="Submit" />
            <Button fx:id="resetBtn" layoutX="165.0" layoutY="225.0" mnemonicParsing="false" text="Reset" />

        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </AnchorPane>
</VBox>
